{
  "name": "cli-packager",
  "description": "Packager for oclif cli with node and npm bundled",
  "version": "1.0.0",
  "author":[ "Jeff Dickey @jdxcode", "Tauheed khan"],
  "bin": {
    "cli-packager": "./bin/run"
  },
  "bugs": "https://github.com/oclif/dev-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.13",
    "@oclif/config": "^1.12.12",
    "@oclif/errors": "^1.2.2",
    "@oclif/plugin-help": "^2.1.6",
    "cli-ux": "^5.2.1",
    "debug": "^4.1.1",
    "fs-extra": "^7.0.1",
    "github-slugger": "^1.2.1",
    "lodash": "^4.17.11",
    "normalize-package-data": "^2.5.0",
    "qqjs": "^0.3.10",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@oclif/plugin-legacy": "^1.1.4",
    "@oclif/test": "^1.2.4",
    "@types/chai": "^4.1.7",
    "@types/execa": "^0.9.0",
    "@types/fs-extra": "^5.0.5",
    "@types/lodash": "^4.14.123",
    "@types/lodash.template": "^4.4.6",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.7",
    "@types/supports-color": "^5.3.0",
    "@types/write-json-file": "^3.2.1",
    "aws-sdk": "^2.443.0",
    "chai": "^4.2.0",
    "conventional-changelog-cli": "^2.0.17",
    "eslint": "^6.6.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^9.2.0",
    "mocha": "^6.1.4",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "files": [
    "/oclif.manifest.json",
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/oclif/dev-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "dirname": "cli-packager",
    "bin": "cli-packager",
    "update": {
      "autoupdate": {
        "rollout": 50,
        "debounce": 60
      },
      "node": {
        "version": "12.12.0"
      }
    },
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "cli-packager",
  "scripts": {
    "prepack": "node ./bin/run manifest",
    "version": "npm run changelog && node ./bin/run readme && git add README.md",
  }
}
